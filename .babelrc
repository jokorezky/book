{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": [
						"last 2 versions"
					]
				},
				"debug": false
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"universal-import",
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-object-rest-spread"
	],
	"env": {
		"development": {
			"plugins": [
				"react-hot-loader/babel"
			]
		}
	}
}
